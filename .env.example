# Chain (84532 = Base Sepolia, 8453 = Base Mainnet)
VITE_CHAIN_ID=84532

# Etherscan API key for contract verification (deployment only)
ETHERSCAN_API_KEY=     

# Required
VITE_WALLETCONNECT_PROJECT_ID=       # WalletConnect Cloud project ID
VITE_DOJO_SCHEMA_UID=                # bytes32 - EAS schema UID after registration
VITE_DOJO_RESOLVER_ADDRESS=          # address - DojoResolver after deployment
VITE_SEKI_TOKEN_ADDRESS=             # address - $SEKI token after creation
VITE_DOJO_TOKEN_ADDRESS=             # address - $DOJO token after creation
VITE_DOJO_FAUCET_ADDRESS=            # address - DojoFaucet contract after deployment
VITE_FAUCET_SECRET=                  # bytes32 - shared secret for faucet proof generation

# Optional
VITE_ALCHEMY_API_KEY=                # Alchemy API key for premium Base RPC

VITE_PINATA_JWT=                     # Pinata JWT for IPFS pinning (cron/build only)

# CDP Server Wallet (weekly distribution operator)
CDP_API_KEY_ID=                      # Coinbase Developer Platform API key ID
CDP_API_KEY_SECRET=                  # CDP API key secret (ES256 private key PEM)
CDP_WALLET_SECRET=                   # CDP wallet secret for server wallet encryption

# Farcaster notifications (weekly distribution cast posting)
NEYNAR_API_KEY=                      # Neynar API key for Farcaster lookups + casting
NEYNAR_SIGNER_UUID=                  # Managed signer UUID for posting casts
